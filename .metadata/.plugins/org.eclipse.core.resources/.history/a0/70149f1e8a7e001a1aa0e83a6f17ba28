package basic;

public class UseVariables {

	
	int x=5;
	int p=4;
	static int z=10;
	String MAX_WIDTH="45";
	final String MIN_WIDTH="35";
			
	public static void main(String[] args) {
		int y=6;
		
System.out.println(UseVariables.z);
		
		
		
		UseVariables objectName= new UseVariables();
		objectName.p=33;
		
		
		System.out.println(objectName.p);
		objectName.MAX_WIDTH="46";
		System.out.println(objectName.MAX_WIDTH);
		System.out.println(objectName.MAX_WIDTH);
		
	}
	
	
	
}
