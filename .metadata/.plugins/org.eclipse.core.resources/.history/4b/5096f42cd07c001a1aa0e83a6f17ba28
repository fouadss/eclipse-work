package basic;

public class NewClass {

	public static void main(String[] args) {
		
		Student fouad= new Student();
		fouad.id=30217;
		fouad.name="fouad";
		fouad.age=26;
		Student mark= new Student();
		mark.id=30218;
		mark.name="mark";
		mark.age=27;
		
		
		System.out.println(fouad.name+" is "+fouad.age+" years old.");	
		
		System.out.println(mark.name +" is"+mark.age + " years old.");
		
		
		
}

}
