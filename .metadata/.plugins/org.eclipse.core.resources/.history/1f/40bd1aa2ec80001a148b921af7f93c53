package firstpack;

public class UseVariables {

	int a; // global variable(non static)
	static int b; //  satatic variable(global)
	
	
	public  void sum() {
	
		a=10;
		b=20;
	int	sum=a+b;
	System.out.println(sum);
	}
	
	
	public static void main(String[] args) {
		  
		int c;
		
		UseVariables obj = new UseVariables();
	
		obj.sum();
		
	}
		
		
		
	
		
		 
		
		
	
	
}
