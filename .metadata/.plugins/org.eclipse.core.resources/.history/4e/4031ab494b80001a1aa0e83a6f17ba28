package firstpackage;

public class NewVariables {

	int z=50;
	
	public int getZ() {
		return z;
	}
	public void setZ(int z) {
		this.z = z;
	}
	static int a=20;
	public static void main(String[] args) {
		int x=90;
		NewVariables obj= new NewVariables();
		System.out.println();
	
	obj.z=80;
	System.out.println(obj.getZ()+x);
	
	
	
	}
}
