package firstpack;

public class UseVariables {
	// declare variables global local static
	//  global non satatic
	String fName;
	String lName;
	int age;
    double salary;
	
    static int id;
    // initialization
	
    
    
    
	public static void main(String[] args) {
		
		UseVariables ob1= new UseVariables();
		
		
		
		
		ob1.fName="mark";
		ob1.lName="william";
		ob1.age=20;
		ob1.salary=4500.235;
		
		UseVariables.id=200;
		
		
		System.out.println("first name is " +ob1.fName);
		
		
	}

}
