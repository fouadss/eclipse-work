package basic;

public class UseStatic {
	static int age=24;
	String computerbrandname="samsung";
	int computerramsize=16;
	double computerprice=1000.500;
	
	   
	
	
	
public static void main(String[] args) {

int x=10;
if(x==1) {
	
	System.out.println("yes x=10");
}
else {
	System.out.println("no x=10");
}
}




}











